{
  "name": "app",
  "private": "true",
  "dependencies": {
    "@hotwired/stimulus": "^3.1.0",
    "@hotwired/turbo-rails": "^7.1.3",
    "@rails/actiontext": "^7.0.4",
    "autoprefixer": "^10.4.10",
    "esbuild": "^0.15.7",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "trix": "^2.0.0-beta.0"
